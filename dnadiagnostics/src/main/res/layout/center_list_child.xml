<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#ffffff"
   >



    <LinearLayout
        android:id="@+id/ll2_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/icon"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/ll_pcenternameF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_seperator"
            android:layout_marginTop="0dp"
            android:weightSum="10"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/id_centeghfjje"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:text="Total Registration"
                android:layout_weight="5"
                android:textAppearance="?android:attr/textAppearanceMedium"

                android:textSize="15sp"
                android:textStyle="normal" />
            <TextView
                android:id="@+id/TotalRegistration_id"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_marginRight="5dp"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#0d261a"
                android:textSize="18sp"
                android:textStyle="bold" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_pnameF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_seperator"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:weightSum="10"
            android:orientation="horizontal">



            <TextView
                android:id="@+id/id_cehdfnteghfjje"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:text="Total Amount"
                android:layout_weight="5"
                android:textAppearance="?android:attr/textAppearanceMedium"

                android:textSize="15sp"
                android:textStyle="normal" />
            <TextView
                android:id="@+id/id_TotalAmount"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_marginRight="5dp"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#0d261a"
                android:textSize="18sp"
                android:textStyle="bold" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_mobilenumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_ageF"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:weightSum="10"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/id_cehdfngfdteghfjje"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:text="Amount Paid"
                android:layout_weight="5"
                android:textAppearance="?android:attr/textAppearanceMedium"

                android:textSize="15sp"
                android:textStyle="normal" />
            <TextView
                android:id="@+id/id_AmountPaid"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_marginRight="5dp"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#0d261a"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imgMapLoction"
                android:layout_width="0dp"
                android:layout_height="25sp"
                android:layout_gravity="center"
                android:layout_weight="0.1"
                android:visibility="gone"
                />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_ageF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_pnameF"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
           android:weightSum="10"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/id_cehdfngfdtdfaeghfjje"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:text="Balance Amount"
                android:layout_weight="5"
                android:textAppearance="?android:attr/textAppearanceMedium"

                android:textSize="15sp"
                android:textStyle="normal" />
            <TextView
                android:id="@+id/id_blnceAmount"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_marginRight="5dp"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#0d261a"
                android:textSize="18sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imgCallbtnTwo"
                android:layout_width="0dp"
                android:layout_height="25sp"
                android:layout_weight="0.1"
                android:visibility="gone"
                />

        </LinearLayout>
    </LinearLayout>
</LinearLayout>

